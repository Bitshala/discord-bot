{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error"
    },
    "overrides": [
      {
        "files": ["*.ts"],
        "parserOptions": {
          "project": ["./tsconfig.json"]
        }
      }
    ]
  }
  